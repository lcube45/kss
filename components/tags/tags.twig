{#
/**
 * @file
 * Displays the tags.
 *
 * Available variables:
 * - tags: tags of the article.
 */
#}

<div class="tags">
    {% for tag in tags %}
          {% include '@mytheme/tag/tag.twig' with
              {
                  label: tag.label,
                  modifier_class: tag.modifier_class
              } only
          %}
    {% endfor %}
</div>
